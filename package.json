{
  "name": "nts-adtools",
  "productName": "National Trade Supply AD Tools",
  "version": "0.1.0",
  "description": "A simple toolset for NTS to work better with AD.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . nts-adtools --platform win32 --arch x64 --out dist/ --overwrite",
    "setup": "electron-installer-windows --src dist/nts-adtools-win32-x64/ --dest dist/installers/"
  },
  "author": "Nicholas Westerhausen",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.0.3",
    "electron-installer-windows": "^2.0.0",
    "electron-packager": "^14.0.5"
  },
  "dependencies": {
    "@mdi/font": "^4.1.95",
    "bootstrap": "^4.3.1",
    "bootswatch": "^4.3.1",
    "datatables.net": "^1.10.19",
    "datatables.net-bs4": "^1.10.19",
    "jquery": "^3.4.1",
    "node-powershell": "^4.0.0",
    "popper.js": "^1.15.0"
  }
}
